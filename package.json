{
  "name": "@pakasa/duuka-whatsapp-button",
  "version": "0.0.4",
  "description": "Adds a whatsapp chat button to the icons menu",
  "packageManager": "yarn@4.9.0",
  "main": "bootstrap.js",
  "files": [
    "graphql",
    "pages",
    "bootstrap.js",
    "constants.js",
    "!**/*.test.*"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "noop": "echo 0",
    "clean": "yarn noop",
    "test": "yarn noop",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "build": "yarn noop",
    "verify": "yarn noop",
    "prepublishOnly": "yarn build && yarn verify",
    "dev": "yarn clean && tsc --noEmitOnError false; tsc-alias --watch",
    "size": "bundlesize",
    "lint": "yarn noop",
    "lint-fix": "yarn noop",
    "prettier": "prettier --log-level warn --write --cache .",
    "prettier-check": "prettier --log-level warn --check --cache ."
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.5",
    "@graphql-tools/schema": "^9.0.3",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.13",
    "clean-css": "^5.3.1",
    "config": "^3.3.6",
    "cross-spawn": "^7.0.6",
    "css-loader": "^6.7.1",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "immer": "^9.0.3",
    "is-object": "^1.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-refresh": "^0.14.0",
    "react-select": "^5.4.0",
    "react-toastify": "^6.2.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "touch": "^3.1.0",
    "uniqid": "^5.3.0",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@eslint/json": "^0.11.0",
    "@eslint/markdown": "^6.3.0",
    "@nx/eslint": "20.7.2",
    "@nx/vite": "20.7.2",
    "@types/config": "^3",
    "@types/lodash": "^4",
    "@vitest/coverage-v8": "^3.1.1",
    "bundlesize": "^0.18.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "nx": "20.7.2",
    "prettier": "^3.5.3",
    "vitest": "^3.1.1"
  }
}
